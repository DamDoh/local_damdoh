# Server
NODE_ENV=development
PORT=8000

# MongoDB
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_secure_password
MONGODB_URI=mongodb://admin:your_secure_password@localhost:27017/damdoh?authSource=admin

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_WINDOW_MS=15m
RATE_LIMIT_MAX_REQUESTS=100

# Cors
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info
PORT=8000
NODE_ENV=development